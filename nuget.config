<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <packageSources>
    <clear />
    <add key="nuget.org" value="https://api.nuget.org/v3/index.json" protocolVersion="3" />
    <add key="github" value="https://nuget.pkg.github.com/delaynore/index.json" />
  </packageSources>
  <packageSourceCredentials>
      <github>
          <add key="Username" value="delaynore" />
          <add key="ClearTextPassword" value="<your token here>" />
      </github>
  </packageSourceCredentials>
  <packageSourceMapping>
    <packageSource key="nuget.org">
      <package pattern="*" />
    </packageSource>
    <packageSource key="github">
      <package pattern="Dlyn.*" />
    </packageSource>
  </packageSourceMapping>
</configuration>
